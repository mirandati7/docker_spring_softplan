{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "front": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/json",
              "src/favicon.ico"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/icheck/skins/flat/blue.css",
              "node_modules/morris.js/morris.css",
              "node_modules/admin-lte/dist/css/AdminLTE.css",
              "node_modules/admin-lte/dist/css/skins/skin-blue.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/flat-icons/ecommerce.css",
              "node_modules/flat-icons/interface.css",
              "node_modules/flat-icons/technology.css",
              "node_modules/flat-icons/creative.css",
              "src/assets/css/icon/dashboard.css",
              "node_modules/primeng/resources/themes/nova-light/theme.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery-ui/jquery-ui.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/raphael/raphael.js",
              "node_modules/morris.js/morris.js",
              "node_modules/jquery-sparkline/jquery.sparkline.js",
              "node_modules/jquery-knob/dist/jquery.knob.min.js",
              "node_modules/moment/moment.js",
              "node_modules/jquery-slimscroll/jquery.slimscroll.js",
              "node_modules/icheck/icheck.js",
              "node_modules/admin-lte/dist/js/adminlte.js",
              "node_modules/fastclick/lib/fastclick.js",
              "node_modules/quill/dist/quill.js",              
              "src/assets/js/scripts.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/jquery.inputmask/dist/jquery.inputmask.bundle.js",
              "node_modules/jquery-circle-progress/dist/circle-progress.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "front:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "front:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "front:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery-ui/jquery-ui.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/raphael/raphael.js",
              "node_modules/morris.js/morris.js",
              "node_modules/jquery-sparkline/jquery.sparkline.js",
              "node_modules/jquery-knob/dist/jquery.knob.min.js",
              "node_modules/moment/moment.js",
              "node_modules/jquery-slimscroll/jquery.slimscroll.js",
              "node_modules/icheck/icheck.js",
              "node_modules/admin-lte/dist/js/adminlte.js",
              "node_modules/fastclick/lib/fastclick.js",
              "node_modules/quill/dist/quill.js",              
              "src/assets/js/scripts.js",
              "node_modules/jquery.inputmask/dist/jquery.inputmask.bundle.js",
              "node_modules/jquery-circle-progress/dist/circle-progress.min.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/icheck/skins/flat/blue.css",
              "node_modules/morris.js/morris.css",
              "node_modules/admin-lte/dist/css/AdminLTE.css",
              "node_modules/admin-lte/dist/css/skins/skin-blue.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/flat-icons/ecommerce.css",
              "node_modules/flat-icons/interface.css",
              "node_modules/flat-icons/technology.css",
              "node_modules/flat-icons/creative.css",
              "src/assets/css/icon/dashboard.css",
              "node_modules/primeng/resources/themes/omega/theme.css"
            ],
            "assets": [
              "src/assets",
              "src/json",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "front-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "front:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "front",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
